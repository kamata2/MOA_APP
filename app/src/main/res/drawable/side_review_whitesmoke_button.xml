<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="2px"
        android:left="2px"
        android:right="2px"
        android:top="2px">
        <shape android:shape="rectangle">
            <stroke
                android:width="3dp"
                android:color="#f2f2f2" />
            <corners android:radius="60dp" />
            <solid android:color="#f2f2f2" />
        </shape>
    </item>
</layer-list>