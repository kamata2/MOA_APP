<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.moaPlatform.moa.util.custom_view.CommonTitleView
        android:id="@+id/titleEatOutStoreDaumMap"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        />

    <RelativeLayout
        android:id="@+id/rlEatOutStoreDaumMapTopInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="13.3dp"
        android:layout_marginRight="13.3dp"
        app:layout_constraintTop_toBottomOf="@id/titleEatOutStoreDaumMap"
        >
        <TextView
            android:id="@+id/tvEatOutStoreDaumMapStoreName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="11.7sp"
            android:textColor="#1c1c1c"
            android:visibility="gone"
            tools:text="TestMessageTestMessageTestMessageTestMessageTestMessageTestMessageTestMessage" />

        <TextView
            android:id="@+id/tvEatOutStoreDaumMapStoreAddr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tvEatOutStoreDaumMapStoreName"
            android:layout_marginTop="8dp"
            android:textSize="11.7sp"
            android:textColor="#1c1c1c"
            tools:text="TestMessageTestMessageTestMessageTestMessageTestMessageTestMessageTestMessage" />

    </RelativeLayout>

    <FrameLayout
        android:id="@+id/flEatOutStoreDaumMapContainter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="13.3dp"
        app:layout_constraintTop_toBottomOf="@id/rlEatOutStoreDaumMapTopInfo" />

</androidx.constraintlayout.widget.ConstraintLayout>