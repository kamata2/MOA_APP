<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="110dp"
    
    >


    <ImageView
        android:id="@+id/subMenuIcon"
        android:layout_width="54dp"
        android:layout_height="59dp"
        android:contentDescription="@string/underline_description"
        android:scaleType="fitXY"
        android:src="@color/black"
        app:layout_constraintBottom_toTopOf="@id/subMenuTitle"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"/>

    <TextView
        android:id="@+id/subMenuTitle"
        style="@style/MoaFont"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="한식"
        android:textColor="@color/black"
        android:textSize="12.7sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/subMenuIcon"
        android:layout_marginTop="11dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>